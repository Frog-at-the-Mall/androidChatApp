<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pd.chatapp">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.androidchatapp.Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.androidchatapp.Login" />
        <activity android:name="com.example.androidchatapp.Chat" />
        <activity android:name="com.example.androidchatapp.Users" />
        <activity android:name="com.example.androidchatapp.appStartActivity" />
        <activity android:name="com.example.androidchatapp.completedPolls" />
        <activity android:name="com.example.androidchatapp.completedLists" />
        <activity android:name="com.example.androidchatapp.announcements_main" />
    </application>

</manifest>